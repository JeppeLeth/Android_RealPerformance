<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dk.bitbreakers.android.realmperformance" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/realm_logo_ss"
        android:label="@string/app_name"
        android:theme="@style/Theme.MyTheme" >
        <activity
            android:name=".ui.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.PerformanceTestActivity"
            android:label="@string/title_activity_performance_test"
            android:parentActivityName=".ui.activity.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="dk.bitbreakers.android.realmperformance.ui.activity.MainActivity" />
        </activity>
    </application>

</manifest>
